{
    "transform": {
        "^.+\\.(t|j)s?$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "moduleFileExtensions": [
        "ts",
        "js",
        "json",
        "node"
    ],
    "setupFilesAfterEnv": [
        "<rootDir>/testHelper/setupTests.tsx"
    ],
    "moduleNameMapper": {
        "^src/(.*)$": "<rootDir>/src/$1"
    }
}